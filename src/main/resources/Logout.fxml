<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.benjazzy.mixchat.controller.LogoutController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Pane prefHeight="69.0" prefWidth="323.0">
            <children>
                <TextArea editable="false" focusTraversable="false" mouseTransparent="true" prefHeight="38.0" prefWidth="100.0" text="Are you sure you want to logout?" />
                <Button fx:id="Done" layoutX="140" layoutY="38.0" mnemonicParsing="false" onAction="#Logout" text="Logout" />
                <Button fx:id="Cancel" layoutX="140" layoutY="38.0" mnemonicParsing="false" onAction="#Cancel" text="Cancel" />
            </children>
        </Pane>
    </children>
</AnchorPane>
