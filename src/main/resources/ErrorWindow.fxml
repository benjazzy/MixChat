<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.benjazzy.mixchat.controller.ErrorController">
    <children>
        <Pane prefHeight="69.0" prefWidth="323.0">
            <children>
                <TextArea editable="false" focusTraversable="false" mouseTransparent="true" prefHeight="38.0" prefWidth="323.0" text="Invalid channel name. Please check spelling and try again!" />
                <Button fx:id="errorWindowDone" layoutX="140" layoutY="38.0" mnemonicParsing="false" onKeyPressed="#handleConnectEnter" onAction="#ErrorWindowDone" text="Done" />
            </children>
        </Pane>
    </children>
</AnchorPane>